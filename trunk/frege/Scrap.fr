module Scrap where
import frege.IO

--Reads a line from standard input device
main _ = do
  isin  <- stdin
  isrin <- IO.InputStreamReader.new isin
  brin <- IO.BufferedReader.fromISR isrin
  line <- brin.readLine
  println line


